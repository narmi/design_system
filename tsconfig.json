{
  "compilerOptions": {
    "alwaysStrict": false,
    "declaration": true,
    "declarationMap": true,
    "importHelpers": true,
    "inlineSources": true,
    "isolatedModules": true,
    "lib": [
        "ES2022",
        "dom"
    ],
    "moduleResolution": "node10",
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "emitDeclarationOnly": true,
    "outDir": "./dist/types",
    "ignoreDeprecations": "5.0"
  },
  "files": [
    "./src/index.ts",
    "./src/types/Icon.types.ts",
    "./src/Slider/Thumbs.tsx",
    "./src/Slider/index.tsx",
    "./src/SeparatorList/index.tsx"
  ],
  "include": [
    "src/Alert/index.d.ts",
    "src/index.ts",
    "src/types/*.ts",
    "src/Slider/*.tsx",
    "src/SeparatorList/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.stories.tsx"
  ]
}